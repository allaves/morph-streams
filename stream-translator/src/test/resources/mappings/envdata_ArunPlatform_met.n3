# ClassMap to build an observation, based on "Val1" column of "envdata_ArunPlatform_met" table
map:ArunPlatform_Val1 a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "observation/Val1/@@envdata_ArunPlatform_met.timestamp@@";
	d2rq:class obs:observation;
	d2rq:classDefinitionLabel "observation";
	.	
map:ArunPlatform_Val1_observationResultTime a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property obs:observationResultTime;
	.
map:ArunPlatform_Val1_timeInstant a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property time:Instant;
	d2rq:belongsToPropertyBridge map:ArunPlatform_Val1_observationResultTime;
	.
map:ArunPlatform_Val1_dateTime a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property time:inXSDDateTime;
	d2rq:belongsToPropertyBridge map:ArunPlatform_Val1_timeInstant;
	d2rq:column "envdata_ArunPlatform_met.0";
	.
map:ArunPlatform_Val1_procedure a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property obs:procedure;
	d2rq:tableName "envdata_ArunPlatform_met";
	.
map:ArunPlatform_Val1_observedProperty a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property obs:observedProperty;
	d2rq:columnHeading "envdata_ArunPlatform_met.5";
	.
map:ArunPlatform_Val1_featureOfInterest a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property obs:featureOfInterest;
	d2rq:constantValue "Blah";
	.
map:ArunPlatform_Val1_observationResult a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property obs:observationResult;
	.
map:ArunPlatform_Val1_uom a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property obs:uom;
	d2rq:constantValue "urn:ogc:def:uom:OGC:m";
	d2rq:belongsToPropertyBridge map:ArunPlatform_Val1_observationResult;
	.
map:ArunPlatform_Val1_ValueThing a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:ArunPlatform_Val1;
	d2rq:property obs:ValueThing;
	d2rq:column "envdata_ArunPlatform_met.5";
	d2rq:belongsToPropertyBridge map:ArunPlatform_Val1_observationResult;
	.
